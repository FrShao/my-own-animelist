{
  "name": "app-anime",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write .",
    "css-build": "node-sass --omit-source-map-url sass/bulma_styles.scss static/bulma.css",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch",
    "prepare": "npm run smui-theme-light",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme"
  },
  "devDependencies": {
    "@rgossiaux/svelte-headlessui": "^1.0.2",
    "@rollup/plugin-dynamic-import-vars": "^2.0.1",
    "@smui/button": "^7.0.0-beta.0",
    "@smui/fab": "^7.0.0-beta.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-netlify": "^1.0.0-next.84",
    "@sveltejs/kit": "next",
    "@tsconfig/svelte": "^3.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^18.11.13",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "rollup-plugin-svelte": "^7.1.0",
    "smui-theme": "^7.0.0-beta.0",
    "svelte": "^3.53.1",
    "svelte-check": "^2.7.1",
    "svelte-grid": "^5.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^3.1.0"
  },
  "type": "module",
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "@svelteuidev/core": "^0.7.7",
    "bcrypt": "^5.1.0",
    "bulma": "^0.9.4",
    "cloudinary": "^1.33.0",
    "locally-unique-id-generator": "^0.1.5",
    "node-sass": "^8.0.0",
    "objection": "^0.4.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sqlite3": "^5.1.3",
    "svelma": "^0.4.5",
    "svelte-grid-extended": "^0.1.0",
    "svelte-waypoint": "^0.1.4"
  },
  "description": "Everything you need to build a Svelte project, powered by [`create-svelte`](https://github.com/sveltejs/kit/tree/master/packages/create-svelte).",
  "main": "rollup.config.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
